<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<TableView fx:id="table" xmlns="http://javafx.com/javafx"
           xmlns:fx="http://javafx.com/fxml"
           fx:controller="main.controller.table.OrderDetailsTableController">
    <columns>
        <TableColumn fx:id="oorderIdColumn" minWidth="-1.0" prefWidth="500.0" text="id zamówienia"/>
        <TableColumn fx:id="beverageIdColumn" minWidth="-1.0" prefWidth="500.0" text="id napoju"/>
        <TableColumn fx:id="quantityColumn" minWidth="-1.0" prefWidth="500.0" text="ilość"/>
    </columns>
    <columnResizePolicy>
        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
    </columnResizePolicy>
</TableView>