<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.*?>
<TableView fx:id="table" xmlns="http://javafx.com/javafx"
           xmlns:fx="http://javafx.com/fxml"
           fx:controller="main.controller.table.OorderTableController">
    <columns>
        <TableColumn fx:id="idColumn" minWidth="-1.0" prefWidth="500.0" text="id zamówienia"/>
        <TableColumn fx:id="customerIdColumn" minWidth="-1.0" prefWidth="500.0" text="id klienta"/>
        <TableColumn fx:id="employeeIdColumn" minWidth="-1.0" prefWidth="500.0" text="id pracownika"/>
        <TableColumn fx:id="orderDateColumn" minWidth="-1.0" prefWidth="500.0" text="data zamówienia"/>
    </columns>
    <columnResizePolicy>
        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
    </columnResizePolicy>
</TableView>