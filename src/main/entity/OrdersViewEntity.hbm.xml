<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="main.entity.OrdersViewEntity" table="ordersView" schema="dbo" catalog="wholesale">
        <property name="id">
            <column name="id" sql-type="int"/>
        </property>
        <property name="firstName">
            <column name="firstName" sql-type="nvarchar(30)" length="30"/>
        </property>
        <property name="lastName">
            <column name="lastName" sql-type="nvarchar(30)" length="30"/>
        </property>
        <property name="orderDate">
            <column name="orderDate" sql-type="datetime"/>
        </property>
    </class>
</hibernate-mapping>