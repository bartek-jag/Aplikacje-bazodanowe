<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="main.model.UsersEntity" table="users" schema="dbo" catalog="wholesale">
        <id name="login">
            <column name="login" sql-type="nchar(25)" length="25"/>
        </id>
        <property name="password">
            <column name="password" sql-type="nchar(128)" length="128"/>
        </property>
    </class>
</hibernate-mapping>